{
	"expo":{
	   "name":"batapp",
	   "slug":"batapp",
	   "version":"1.0.0",
	   "orientation":"portrait",
	   "icon":"./assets/images/icons/icon.png",
	   "userInterfaceStyle":"dark",
	   "backgroundColor":"#00000",
	   "splash":{
		  "image":"./assets/images/splash.png",
		  "resizeMode":"cover",
		  "backgroundColor":"#00000"
	   },
	   "updates":{
		  "fallbackToCacheTimeout":0,
		  "enabled":true,
		  "checkAutomatically":"ON_LOAD",
		  "url":""
	   },
	   "ios":{
		  "buildNumber":"1.0.0",
		  "supportsTablet":true,
		  "bundleIdentifier":"",
		  "googleServicesFile":"./GoogleService-Info.plist",
		  "infoPlist":{
			 "NSCameraUsageDescription":"This app requires access to the camera to take photos.",
			 "NSPhotoLibraryUsageDescription":"This app requires access to the photo library to upload photos.",
			 "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow batapp to access your location.",
			 "NSLocationAlwaysUsageDescription": "Allow batapp to access your location.",
			 "NSLocationWhenInUseUsageDescription": "Allow batapp to access your location.",
			 "NSUserTrackingUsageDescription": "This identifier will be used to track your activity for personalized ads.",
			 "NSMicrophoneUsageDescription": "This app requires access to the microphone to record your voice for verification.",
			 "SKAdNetworkItems": [
				{
					"SKAdNetworkIdentifier": ""
				},
				{
					"SKAdNetworkIdentifier": ""
				},
				{
					"SKAdNetworkIdentifier": ""
				}
				]
		  },
		  "config": {
			"usesNonExemptEncryption":false
		  }
	   },
	   "android":{
		  "versionCode":1,
		  "adaptiveIcon":{
			 "foregroundImage":"./assets/images/icons/adaptive-icon.png",
			 "backgroundImage":"./assets/images/icons/background.png",
			 "monochromeImage":"./assets/images/icons/monochrome.png",
			 "backgroundColor":"#00000"
		  },
		  "googleServicesFile":"./google-services.json",
		  "permissions":[
			 "READ_EXTERNAL_STORAGE",
			 "WRITE_EXTERNAL_STORAGE",
			 "CAMERA",
			 "RECORD_AUDIO",
			 "ACCESS_COARSE_LOCATION",
			 "ACCESS_FINE_LOCATION",
			 "INTERNET",
			 "com.android.vending.BILLING",
			 "com.google.android.gms.permission.AD_ID"
		  ],
		  "package":""
	   },
	   "web":{
		  "favicon":"./assets/images/icons/favicon.png"
	   },
	   "extra":{
		  "eas":{
			 "projectId":""
		  }
	   },
	   "notification":{
		  "color":"#00000",
		  "iosDisplayInForeground":true
	   },
	   "plugins":[
		  "@react-native-firebase/app",
		  "@react-native-firebase/crashlytics",
		  "@react-native-firebase/messaging",
		  [
			 "expo-build-properties",
			 {
				"ios":{
				   "useFrameworks":"static"
				}
			 }
		  ]
	   ],
	   "owner":"kodelle",
	   "runtimeVersion":"1.0.0"
	}
}